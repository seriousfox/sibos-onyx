<script type="text/javascript"> 
  $("#from li").draggable({
      appendTo: "body",
      cursor: "move",
      // helper: 'clone',
      revert: "invalid"
  });


  $("#to li").droppable({
    accept: "#from li",
    activeClass: "ui-state-default",
    hoverClass: "ui-state-hover",
    drop: function(event, ui) {        
      $(ui.helper).fadeOut()
    }
  });
</script>
